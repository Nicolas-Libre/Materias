programa ejemplo
procesos
  proceso tomarFlores(E flores: numero; E AvActual:numero; E CaActual:numero)
  variables
    f:numero
  comenzar
    f:=0
    mientras (f<>flores)
      BloquearEsquina (21,21)
      Pos (21,21)
      tomarFlor
      f:=f+1
      Pos (AvActual,CaActual)
      LiberarEsquina (21,21)
  fin
  proceso tomarPapeles (E papeles:numero; E AvActual:numero; E CaActual:numero)
  variables
    p:numero
  comenzar
    p:=0
    mientras (p<>papeles)
      BloquearEsquina (22,22)
      Pos (22,22)
      tomarPapel
      p:=p+1
      Pos (AvActual,CaActual)
      LiberarEsquina (22,22)
  fin   
  proceso hacerRectangulo (E alto:numero; E ancho:numero; ES cantObj:numero)
  variables
    flores:numero
    papeles:numero
    AvActual:numero
    CaActual:numero
  comenzar
    repetir 2
      repetir alto
        AvActual:=PosAv
        CaActual:=PosCa
        Random (flores,1,3)
        Random (papeles,1,3)
        tomarFlores (flores,AvActual,CaActual)
        tomarPapeles (papeles,AvActual,CaActual)
        repetir flores
          depositarFlor
          cantObj:=cantObj+1
        repetir papeles
          depositarPapel
          cantObj:=cantObj+1
        mover
      derecha
      repetir ancho
        AvActual:=PosAv
        CaActual:=PosCa
        Random (flores,1,3)
        Random (papeles,1,3)
        tomarFlores (flores,AvActual,CaActual)
        tomarPapeles (papeles,AvActual,CaActual)
        repetir flores
          depositarFlor
          cantObj:=cantObj+1
        repetir papeles
          depositarPapel
          cantObj:=cantObj+1
        mover
      derecha
  fin
areas
  AR1: AreaP (1,1,20,20)
  AR2: AreaP (21,1,40,20)
  ARC: AreaP (23,23,23,23)
  EsquinaPap: AreaPC (21,21,21,21)
  EsquinaFlo: AreaPC (22,22,22,22)
robots
  robot robotTrabajador
  variables
    soy:numero
    ancho:numero
    alto:numero
    cantObj:numero
  comenzar
    RecibirMensaje (soy,RC)
    EnviarMensaje (soy,RC)
    RecibirMensaje (alto,RC)
    RecibirMensaje (ancho,RC)
    hacerRectangulo (alto,ancho,cantObj)
    EnviarMensaje (soy,RC)
    EnviarMensaje (cantObj,RC)
  fin
  robot robotCoordinador
  variables
    soy:numero
    alto:numero
    ancho:numero
    primero,ultimo:numero
    cantObj1,cantObj2:numero
  comenzar
    EnviarMensaje (1,r1)
    EnviarMensaje (2,r2)
    repetir 2
      RecibirMensaje (soy,*)
      Random (alto,1,19)
      Random (ancho,1,19)
      si (soy=1)
        EnviarMensaje (alto,r1)
        EnviarMensaje (ancho,r1)
      sino
        EnviarMensaje (alto,r2)
        EnviarMensaje (ancho,r2)
    repetir 2
      RecibirMensaje (soy,*)
      si (soy=1)
        RecibirMensaje (cantObj1,r1)
        si (primero<>2)
          primero:=1
        sino
          ultimo:=1
      sino
        RecibirMensaje (cantObj2,r2)
        si (primero<>1)
          primero:=2
        sino
          ultimo:=2
    si (primero=1)
      Informar ('el robottUno llego primero con objetos iguales a ', cantObj1)
    sino
      Informar ('el robottDos llego primero con objetos iguales a ', cantObj2)
  fin
  
variables
  r1,r2: robotTrabajador
  RC: robotCoordinador
comenzar
  AsignarArea(r1, AR1)
  AsignarArea(r2, AR2)
  AsignarArea(r1, EsquinaFlo)
  AsignarArea(r2, EsquinaFlo)
  AsignarArea(r1, EsquinaPap)
  AsignarArea(r2, EsquinaPap)
  AsignarArea(RC, ARC)
  Iniciar(r1, 1,1)
  Iniciar(r2, 21,1)
  Iniciar(RC, 23,23)
fin