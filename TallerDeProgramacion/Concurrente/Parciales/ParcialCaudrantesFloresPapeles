programa ejemplo
procesos
  proceso cargarBolsa (E AvActual:numero; E CaActual:numero; E cantFlores:numero; E cantPapeles:numero)
  variables
    totalF,totalP:numero
  comenzar
    totalF:=0
    totalP:=0
    mientras ((totalF<cantFlores) | (totalP<cantPapeles))
      si (totalF<>cantFlores)
        BloquearEsquina (21,21)
        Pos (21,21)
        tomarFlor
        totalF:=totalF+1
        Pos (AvActual,CaActual)
        LiberarEsquina (21,21)
      si (totalP<>cantPapeles)
        BloquearEsquina (22,22)
        Pos (22,22)
        tomarPapel
        totalP:=totalP+1
        Pos(AvActual,CaActual)
        LiberarEsquina (22,22)
  fin
  proceso hacerRectangulo(E largo: numero; E ancho:numero; ES cantTotal:numero)
  variables
    cantFlores,cantPapeles:numero    
  comenzar
    repetir 2
      repetir largo
        Random (cantFlores,1,3)
        Random (cantPapeles,1,3)
        cargarBolsa (PosAv,PosCa, cantFlores,cantPapeles)
        repetir cantFlores
          depositarFlor
          cantTotal:=cantTotal+1
        repetir cantPapeles
          depositarPapel
          cantTotal:=cantTotal+1
        mover
      derecha
      repetir ancho
        Random (cantFlores,1,3)
        Random (cantPapeles,1,3)
        cargarBolsa (PosAv,PosCa, cantFlores,cantPapeles)
        repetir cantFlores
          depositarFlor
          cantTotal:=cantTotal+1
        repetir cantPapeles
          depositarPapel
          cantTotal:=cantTotal+1
        mover
      derecha
  fin
areas
  AR1: AreaP (1,1,20,20)
  AR2: AreaP (21,1,40,20)
  EsquinaF: AreaPC (21,21,21,21)
  EsquinaP: AreaPC (22,22,22,22)
  ARC: AreaP (23,23,23,23)
robots
  robot robotTrabajador
  variables
    AvActual,CaActual:numero
    cantTotal:numero
    empiezo:boolean
    soy:numero
    ancho,largo:numero
  comenzar
    AvActual:=PosAv
    CaActual:=PosCa
    RecibirMensaje (soy,RC)
    RecibirMensaje (ancho,RC)
    RecibirMensaje (largo,RC)
    EnviarMensaje (soy,RC)
    RecibirMensaje (empiezo,RC)
    hacerRectangulo (largo,ancho, cantTotal)
    EnviarMensaje (soy,RC)
    EnviarMensaje (cantTotal,RC)
  fin
  robot robotCoordinador
  variables
    soy:numero
    tot1,tot2:numero
    ancho,largo:numero
    primero,ultimo:numero
  comenzar
    EnviarMensaje (1,r1)
    EnviarMensaje (2,r2)
    Random (ancho,1,20)
    Random (largo,1,20)
    EnviarMensaje (ancho,r1)
    EnviarMensaje (largo,r1)
    Random (ancho,1,20)
    Random (largo,1,20)
    EnviarMensaje (ancho,r2)
    EnviarMensaje (largo,r2)
    repetir 2
      RecibirMensaje (soy,*)
      si (soy=1)
        EnviarMensaje (V,r1)
      sino
        EnviarMensaje (V,r2)
    repetir 2
      RecibirMensaje (soy,*)
      si (soy=1)
        RecibirMensaje (tot1,r1)
        si (primero<>2)
          primero:=1
          ultimo:=2
      sino
        RecibirMensaje (tot2,r2)
        si (primero<>1)
          primero:=2
          ultimo:=1 
    Informar ('el primer robott fue el ', primero)
    si (primero=1)
      Informar ('junto flores iguales a ', tot1)
      Informar ('el ultimo robott fue el ', ultimo)
      Informar ('y junto flores iguales a ', tot2)
    sino
      Informar ('junto flores iguales a ', tot2)
      Informar ('el ultimo robott fue el ', ultimo)
      Informar ('y junto flores iguales a ', tot1)
  fin  
variables
  r1,r2: robotTrabajador
  RC: robotCoordinador
comenzar
  AsignarArea(r1, AR1)  
  AsignarArea(r2, AR2)
  AsignarArea(r1, EsquinaF)  
  AsignarArea(r2, EsquinaF)
  AsignarArea(r1, EsquinaP)  
  AsignarArea(r2, EsquinaP)
  AsignarArea(RC, ARC)
  Iniciar(r1, 1,1)
  Iniciar(r2, 21,1)
  Iniciar(RC, 23,23)
fin